editor	/usr/bin/vi
mail	/var/mail
mandir	/usr/local/man
manuals	local
mts	sendmail
options	BSD42
options	MORE='"/usr/local/bin/less"'
options	MSGPROT='"0600"'SHADOW
options	SYS5
options	SVR4
options	SYS5DIR
options	TYPESIG=void
options	UK
options	on
sprintf	int
cc	cc
ccoptions -O
ldoptions -s
curses	-lcurses
sharedlib sys5
