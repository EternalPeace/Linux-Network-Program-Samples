lp|ucbvax|vx|vax|ucbvax line printer:\
	:lp=:rm=ucbvax:lf=/usr/adm/lpd-errs:\
	:rg=printers:
ap|arpa|ucbarpa|ucbarpa line printer:\
	:lp=:rm=ucbarpa:rp=arpa:sd=/usr/spool/apd:lf=/usr/adm/lpd-errs:\
	:rg=printers:
varian|va|Benson Varian:\
	:lp=:rm=ucbrenoir:rp=varian:sd=/usr/spool/vad:lf=/usr/adm/lpd-errs:\
	:rg=printers:
versatec|vp|Versatec:\
	:lp=:rm=ucbdegas:rp=versatec:sd=/usr/spool/vpd:lf=/usr/adm/lpd-errs:\
	:rg=printers:
imagen|ip|Imagen laser printer:\
	:lp=:rm=ucbarpa:rp=imagen:sd=/usr/spool/ipd:lf=/usr/adm/lpd-errs:\
	:rg=printers:
dp|dandyprinter|xerox dandelion laser printer:\
	:lp=/dev/null:sd=/usr/spool/dpd:lf=/usr/adm/lpd-errs:\
	:tf=/usr/local/lib/xtcat:nf=/usr/local/lib/xcat:af=/usr/adm/xacct:sh:
op|officeprinter|xerox office laser printer:\
	:lp=/dev/null:sd=/usr/spool/opd:lf=/usr/adm/lpd-errs:\
	:rg=oprint:if=/usr/local/lib/ascf.op\
	:nf=/usr/local/lib/xcat2:af=/usr/adm/xacct2:sh:
