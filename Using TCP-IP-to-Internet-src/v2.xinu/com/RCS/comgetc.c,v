#include <conf.h>
#include <kernel.h>
#include <io.h>

/*------------------------------------------------------------------------
 *  comgetc -- read a character from a serial line
 *------------------------------------------------------------------------
 */
int
comgetc(pdev )
struct	devsw	*pdev;
{
	return SYSERR;
}


/*------------------------------------------------------------------------
 *  comread -- read from a serial line
 *------------------------------------------------------------------------
 */
int
comread(pdev, buf, count)
struct	devsw	*pdev;
char *buf;
int count;
{
	return SYSERR;
}
