1.4
log
@pre-3e code
@
text
@/* ue_int.s - _ue_int */

#include <icu.s>

		.text
		.globl	_ue_int
_ue_int:
		cli
		pushal

		movb	$EOI,%al	/* re-enable the device */
		outb	%al,$OCW1_2

		call	_ue_intr

		popal
		sti
		iret
@


1.3
log
@this calls disable/restore, but stacks up on interrupts after
the restore, because it does an sti. changing disable/restore to
not mess with the IE bit.
@
text
@d8 1
a8 8
		pushl	%ebp
		movl	%esp,%ebp
		subl	$4,%esp

		leal	-4(%ebp),%eax
		pushl	%eax
		call	_disable

d11 1
a11 1
		movb	$EOI,%al
d17 1
a17 6

		call	_restore	/* arg still on stack from disable */
		addl	$4,%esp

		movl	%ebp,%esp
		popl	%ebp
@


