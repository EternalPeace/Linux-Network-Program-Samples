@/* seek.c seek */

#include <conf.h>
#include <kernel.h>
#include <io.h>

/*------------------------------------------------------------------------
 *  seek  --  position a device (very common special case of control)
 *------------------------------------------------------------------------
 */
seek(descrp, pos)
int descrp;
long pos;
{
	struct	devsw	*devptr;

	if (isbaddev(descrp) )
		return(SYSERR);
	devptr = &devtab[descrp];
        return( (*devptr->dvseek)(devptr,pos) );
}
@
