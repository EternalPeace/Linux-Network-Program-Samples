#include <conf.h>
#include <kernel.h>
#include <io.h>

/*------------------------------------------------------------------------
 *  congetc -- read a character from the SUN physical keyboard
 *------------------------------------------------------------------------
 */
int
congetc(devptr )
struct	devsw	*devptr;
{
	return SYSERR;
}


/*------------------------------------------------------------------------
 *  conread -- read from the SUN physical keyboard
 *------------------------------------------------------------------------
 */
int
conread(devptr, buff, count )
struct	devsw	*devptr;
char *buff;
int count;
{
	return SYSERR;
}
